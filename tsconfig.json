{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "incremental": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "esModuleInterop": true,
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "strictNullChecks": true,
      "jsx": "preserve",
      "baseUrl": ".",
    },
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx", "postcss.config.js", "tailwind.config.js", "next.config.js",
    ],
    "exclude": [
      "node_modules",
      "*.config.js",
    ],
  }
  